{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/(tabs)/settings.tsx",
    "app/(tabs)/home/session.tsx"
  ],
  "allowJs": true,
  "skipLibCheck": true,
  "resolveJsonModule": true,
  "exclude": ["node_modules", "babel.config.js", "metro.config.js"]
}
